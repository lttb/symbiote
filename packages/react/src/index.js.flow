/* @flow */

import type {ComponentType, ElementConfig} from 'react';

declare export function createRegistry(): {
    RegistryProvider: ComponentType<{}>,
    RegistryContext: React$Context<{}>,
    registry: {},
    register: <Context, Base>(
        ComponentType<Base>,
        Context,
    ) => <Current>(
        Current,
    ) => ComponentType<
        $Exact<{
            ...$Exact<$Diff<ElementConfig<Current>, Context>>,
            ...$Diff<
                ElementConfig<Current>,
                $Diff<ElementConfig<Current>, Context>,
            >,
        }>,
    >,
};
